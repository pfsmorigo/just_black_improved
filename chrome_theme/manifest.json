{
	"manifest_version": 2,
	"name": "Dark Simple Chrome Theme",
	"version": "0.1",
	"description": "A dark and simple theme that I use for Chrom{ium,e}",
	"author": "Paulo Flabiano Smorigo|pfsmorigo|pfsmorigo",
	"theme": {
		"colors": {
			"bookmark_text": [ 240, 240, 240 ],
			"control_background": [ 0, 0, 0, 0 ],
			"frame": [ 50, 50, 50 ],
			"frame_inactive": [ 50, 50, 50 ],
			"frame_incognito": [ 50, 50, 50 ],
			"frame_incognito_inactive": [ 50, 50, 50 ],
			"ntp_background": [ 0, 0, 0 ],
			"ntp_header": [ 75, 140, 220 ],
			"ntp_link": [ 200, 200, 200 ],
			"ntp_section": [ 229, 239, 254 ],
			"ntp_section_link": [ 6, 55, 116 ],
			"ntp_section_text": [ 0, 0, 0 ],
			"ntp_text": [ 200, 200, 200 ],
			"tab_background_text": [ 64, 64, 64 ],
			"tab_text": [ 0, 0, 0 ],
			"toolbar": [ 50, 50, 50 ],
			"button_background": [150, 150, 150]
		},
		"images": {
			"theme_toolbar": "images/toolbar.png",
			"theme_tab_background": "images/tab_background.png",
			"theme_ntp_background" : "images/ntp_background.jpg",
			"theme_frame": "images/theme_frame.png"
		},
		"properties": {
			"ntp_background_alignment": "top",
			"ntp_background_repeat": "no-repeat"
		}
	}
}
